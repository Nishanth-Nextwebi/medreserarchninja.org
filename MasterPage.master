            <script>
                $(document).ready(function () {

                    $(document.body).on("click", ".btnmail", function (e) {
                        e.preventDefault();
                        var mail = $("#txtmail").val();
                        $(".spnmail").html("");
                        var count = 1;

                        if (mail === "") {
                            count = 0;
                            $(".spnmail").html("Please enter Email Id");
                        }
                        if (count === 1) {
                            $.ajax({
                                type: 'POST',
                                url: "/Default.aspx/EmailSubscriptions",
                                contentType: 'application/json; charset=utf-8',
                                dataType: "json",
                                data: JSON.stringify({ mail: mail }),
                                success: function (data2) {
                                    if (data2.d === "Success") {
                                        Snackbar.show({ pos: 'top-right', text: 'Thank you for subscribing.', actionTextColor: '#fff', backgroundColor: '#ff7f3e' });
                                        $("#txtmail").val("");
                                    } else if (data2.d === "Exists") {
                                        Snackbar.show({ pos: 'top-right', text: 'Email already exists.', actionTextColor: '#fff', backgroundColor: '#ff7f3e' });
                                    } else {
                                        Snackbar.show({ pos: 'top-right', text: 'Oops...! There is some problem right now. Please try again later.', actionTextColor: '#fff', backgroundColor: '#ff7f3e' });
                                    }
                                }
                            });
                        }
                    });
                });
